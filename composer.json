{
    "name": "mad-web/social-auth",
    "type": "library",
    "description": "Easy social auth integration with a lot of available providers",
    "keywords": [
        "mad-web",
        "social",
        "auth",
        "laravel",
        "socialite",
        "login",
        "signin"
    ],
    "homepage": "https://github.com/mad-web/social-auth",
    "license": "MIT",
    "authors": [
        {
            "name": "Mad Web",
            "email": "madweb.dev@gmail.com",
            "homepage": "https://github.com/mad-web",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~7.0",
        "illuminate/support": "^5.4",
        "illuminate/events": "^5.4",
        "illuminate/database": "^5.4",
        "illuminate/routing": "^5.4",
        "illuminate/broadcasting": "^5.4",
        "socialiteproviders/manager": "~2.0 || ~3.0"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.8",
        "phpunit/phpunit": "^5.7",
        "orchestra/testbench": "^3.4"
    },
    "autoload": {
        "psr-4": {
            "MadWeb\\SocialAuth\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MadWeb\\SocialAuth\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    }
}
