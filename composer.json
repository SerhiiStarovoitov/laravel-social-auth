{
    "name": "zfort/social-auth",
    "type": "library",
    "description": "Easy social auth integration with a lot of available providers",
    "keywords": [
        "zfort",
        "social",
        "auth",
        "laravel",
        "socialite"
    ],
    "homepage": "https://github.com/zfort/social-auth",
    "license": "MIT",
    "authors": [
        {
            "name": "zFort Group",
            "email": "developer@zfort.com",
            "homepage": "https://zfort.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~7.0",
        "illuminate/support": "^5.4",
        "illuminate/events": "^5.4",
        "illuminate/database": "^5.4",
        "illuminate/routing": "^5.4",
        "illuminate/broadcasting": "^5.4",
        "socialiteproviders/manager": "~2.0 || ~3.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "~4.0||~5.0"
    },
    "autoload": {
        "psr-4": {
            "ZFort\\SocialAuth\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ZFort\\SocialAuth\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
